<h2>Hello world </h2>

<%= form_for :contact , url: contacts_path do |f|%>
    <p>
        <%= f.label :email %><br />
        <%= f.text_field :email %>
    </p>
    <p>
        <%= f.label :message %><br />
        <%= f.text_area :message %>
    </p>
    <p>
        <%= f.submit 'send message'%>
    </p>
      
<%end%>